    "jest-junit-reporter": "^1.1.0",
    "jscodeshift": "0.5.0",
    "patch-package": "6.1.2",
    "prettier": "^1.6.0",
    "prettier-eslint-cli": "5.0.0",
    "react-test-renderer": "16.8.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "3.8.3",
    "zip-a-folder": "0.0.6"
  },
  "resolutions": {
    "lodash": "^4.17.14",
    "lodash.merge": "4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "e2e/Exponent-2.11.1.app",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "ios.sim2": {
        "binaryPath": "e2e/Exponent-2.11.1.app",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  }
