{
  "name": "docdok",
  "version": "21.4.0",
  "release": "pizol",
  "description": "repository for docdok mobile app",
  "author": "Xavier Carpentier <xcapetir@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "main": "node_modules/expo/AppEntry.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e-medicus/mobile-docdok.git"
  },
  "scripts": {
    "test": "node_modules/.bin/jest --config configuration/jest.config.json",
    "lint": "./node_modules/.bin/tslint ./src/**/*.{ts,tsx}",
    "tsc": "node_modules/.bin/tsc --noEmit",
    "prettier:list": "./node_modules/.bin/prettier-eslint \"src/**/*.js\" --single-quote --trailing-comma none --list-different",
    "prettier:write": "./node_modules/.bin/prettier-eslint \"src/**/*.js\" --single-quote --trailing-comma none --write && git add .",
    "check-deps": "npx npm-check-updates -e 2 --dep prod",
    "check-vulnerabilities": "[ `yarn audit --json | jq .data.vulnerabilities.critical | tail -1` -eq 0 ]",
    "check-duplicate": "npx jscpd ./src -s | grep 'Found 0'",
    "cleaning": "./scripts/clean.sh",
    "copyEnv": "./scripts/copy_env.sh",
    "revertEnv": "./scripts/revert_env.sh",
    "android-screenshot": "./scripts/screenshot_android.sh",
    "precommit": "yarn lint && yarn tsc",
    "prepush": "yarn test",
    "expo:login": "./scripts/login_expo.sh",
    "expo:logout": "./scripts/logout_expo.sh",
    "expo:deploy": "./scripts/deploy_to_expo.sh",
    "expo:deploy:it_to_qa": "./scripts/deploy_it_to_qa.sh",
    "expo:deploy:qa_to_demo1": "./scripts/deploy_qa_to_demo1.sh",
    "expo:deploy:qa_to_demo2": "./scripts/deploy_qa_to_demo2.sh",
    "build-apk": "./scripts/build_apk.sh",
    "build-ipa": "./scripts/build_ipa.sh",
    "build-ipa-simu": "./scripts/build_ipa_simu.sh",
    "start": "expo r",
    "start:clean": "expo r -c",
    "start:ios": "expo r -c -i",
    "start:android": "expo r -c -a",
    "e2e": "./scripts/init_e2e.js && rm -rf e2e/screenshots/* && detox test --configuration ios.sim2 --cleanup --artifacts-location ${CIRCLE_ARTIFACTS:='./e2e/screenshots/'} --record-logs all --take-screenshots all",
    "build:simu:qa": "expo build:ios -t simulator --release-channel qa",
    "patch-package": "patch-package",
    "prepare": "yarn patch-package"
  },
  "dependencies": {
    "@applications-developer/react-native-transformable-image": "^0.1.1",
